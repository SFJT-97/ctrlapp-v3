{
  "expo": {
    "name": "control-accion-v2",
    "slug": "control-accion-v2",
    "scheme": "control-accion-v2-scheme",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/ca-registered-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "splash": {
      "image": "./assets/ca-registered-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.humberju.controlaccionv2",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Usaremos Face ID para iniciar sesión rápidamente y mejorar tu experiencia.",
        "aps-environment": "development",
        "UIBackgroundModes": ["remote-notification"],
        "NSPhotoLibraryUsageDescription": "Allow access to your library to select photos and videos.",
        "NSDocumentsFolderUsageDescription": "Allow access to your documents to select media.",
        "NSCameraUsageDescription": "Allow access to your camera to take photos and videos.",
        "NSMicrophoneUsageDescription": "Allow access to your microphone to record videos.",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true,
          "NSExceptionDomains": {
            "137.184.22.126": {
              "NSIncludesSubdomains": false,
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true
            },
            "164.92.67.239": {
              "NSIncludesSubdomains": false,
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true
            },
            "192.168.0.176": {
              "NSIncludesSubdomains": false,
              "NSTemporaryExceptionAllowsInsecureHTTPLoads": true
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/10000920.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.POST_NOTIFICATIONS",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.humberju.controlaccionv2",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(control-accion-v2) to access your microphone."
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "androidGoogleServicesFile": "./android/app/google-services.json"
        }
      ],
      "@react-native-firebase/messaging",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 23
          }
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "af868460-a685-48e5-bdfc-086c33282ee2"
      }
    },
    "owner": "humberju"
  }
}